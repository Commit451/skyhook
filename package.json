{
  "name": "skyhook",
  "version": "0.0.1",
  "description": "Parses webhooks and forwards it in the proper format to Discord",
  "scripts": {
    "tsc": "tsc",
    "build": "npm run tsc",
    "dockerstart": "node dist/index.js",
    "gcp-build": "npm run build",
    "start": "node dist/index.js",
    "buildstart": "npm run build && npm run start",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "lint": "eslint . --ext .js,.ts",
    "deploy": "npm run build && gcloud app deploy",
    "logs": "gcloud app logs tail -s default"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/express": "^4.17.13",
    "@types/gravatar": "^1.8.3",
    "@types/luxon": "^2.0.7",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.9",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.3.0",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "dotenv": "10.0.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "gravatar": "^1.8.2",
    "luxon": "^2.1.1",
    "turndown": "^7.1.1",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  },
  "repository": {
    "url": "https://github.com/Commit451/skyhook"
  },
  "bugs": {
    "url": "https://github.com/Commit451/skyhook/issues"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "skyhook",
    "discord"
  ]
}
